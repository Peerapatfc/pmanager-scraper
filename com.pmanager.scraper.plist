<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>Label</key>
    <string>com.pmanager.scraper</string>
    
    <key>ProgramArguments</key>
    <array>
        <string>/bin/bash</string>
        <string>REPLACE_WITH_PROJECT_PATH/run-scraper.sh</string>
    </array>
    
    <key>StartInterval</key>
    <integer>21600</integer>
    
    <key>RunAtLoad</key>
    <false/>
    
    <key>StandardOutPath</key>
    <string>REPLACE_WITH_PROJECT_PATH/logs/launchd-stdout.log</string>
    
    <key>StandardErrorPath</key>
    <string>REPLACE_WITH_PROJECT_PATH/logs/launchd-stderr.log</string>
    
    <key>WorkingDirectory</key>
    <string>REPLACE_WITH_PROJECT_PATH</string>
</dict>
</plist>
